{
    "name": "Hotpot common code tests",
    "version": "0.0.0",
    "main": {
        "module-name": "Hotpot.js"
    },
    "engines": {
        "node": ">=12.0.0"
    },
    "bugs": {
        "url": "https://github.com/cdot/Hotpot/issues"
    },
    "author": "Crawford Currie",
    "repository": {
        "type": "git",
        "url": "https://github.com/cdot/Hotpot.git"
    },
    "license": "MIT",
    "dependencies": {
        "jquery": "^3.6.1",
        "jquery-touch-events": "^1.0.7",
        "jquery-ui-dist": "^1.13.2",
        "requirejs": "^2.3.6"
    },
    "analyze": true,
    "eslintConfig": {
        "parserOptions": {
            "ecmaVersion": 2017
        },
        "env": {
            "browser": true,
            "jquery": true
        },
        "globals": {
            "module": true,
            "HOTPOT_DEBUG": true
        },
        "rules": {
            "camelcase": 0,
            "curly": 0,
            "dot-notation": 1,
            "eol-last": 0,
            "new-cap": 0,
            "no-alert": 0,
            "no-console": 0,
            "no-debugger": 0,
            "no-eval": 0,
            "no-loop-func": 0,
            "no-new": 0,
            "no-process-exit": 0,
            "no-underscore-dangle": 0,
            "no-unused-vars": 1,
            "no-use-before-define": 0,
            "no-trailing-spaces": 0,
            "quotes": 0,
            "strict": 0
        }
    },
    "devDependencies": {
        "chai": "^4.3.7",
        "chai-http": "^4.3.0",
        "eslint": ">=8.29.0",
        "jsdoc": "^4.0.0",
        "mocha": "^10.1.0",
        "request": "^2.88.2"
    }
}
